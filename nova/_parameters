@namespace: problem

nx                       integer            256

rmin                     real               3.9e8
rmax                     real               4.5e8

# gravity type: 0 = constantr, 1 = 1/r^2, 2 = enclosed mass gravity
g_type                   integer            0

# mass of the star (in grams)
m                        real               2.e33


# core profile isothermal or isentropic?  1 = isothermal, 2 = isentropic
p_type                   integer            1

# core edge: extend initial model with profile? or overwrite profile
# from envelope base
c_edge                   integer            0

# smoothing: 0 = none, 1 = KNRL, 2 = TNH0, 3 = TANH, 4 = TNH2, 5 = GSSN, 6 = EXNP
s_type                   integer            0

# smoothing length
s_length                 real               1.e5

model_file               character          "glasner_nova.dat"
